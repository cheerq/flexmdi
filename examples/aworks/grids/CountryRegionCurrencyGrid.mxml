<?xml version="1.0" encoding="utf-8"?>
<flexmdi:MDIWindow xmlns:mx="http://www.adobe.com/2006/mxml" width="400" height="300" creationComplete="onCreationComplete()"
	xmlns:flexmdi="http://flexmdi.googlecode.com/">
	
	<mx:Script>
		<![CDATA[
			import aworks.vo.Contact;
			import aworks.core.DataTable;
			import mdi.containers.MDIWindow;
			
			[Bindable]
			public var countryregioncurrencys : DataTable = new DataTable("getCountryRegionCurrencys",aworks.vo.CountryRegionCurrency);
			
			public function onCreationComplete():void
			{
				countryregioncurrencys.invoke();
			}
			
		]]>
	</mx:Script>
	
	<mx:DataGrid dataProvider="{this.countryregioncurrencys.rows}" width="100%" height="100%">
		<mx:columns>
		
			<mx:DataGridColumn dataField="COUNTRYREGIONCODE" headerText="CountryRegionCode" />
			<mx:DataGridColumn dataField="CURRENCYCODE" headerText="CurrencyCode" />
			<mx:DataGridColumn dataField="MODIFIEDDATE" headerText="ModifiedDate" />
			
		</mx:columns>
	</mx:DataGrid>
	
	<mx:ControlBar width="100%">
		
		<mx:Button click="countryregioncurrencys.invoke()" label="refresh" />
		<mx:Text text="{countryregioncurrencys.rows.length} Record(s)" />
		<mx:Spacer width="100%" />
		
		
	</mx:ControlBar>
	
</flexmdi:MDIWindow>



